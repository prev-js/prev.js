{
  "name": "previous.js",
  "version": "0.4.0",
  "description": "The prev generation React Framework",
  "bin": {
    "prev": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "bump": "bump -c bump.config.json"
  },
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./router": {
      "types": "./dist/export-router.d.ts",
      "require": "./dist/export-router.js",
      "import": "./dist/export-router.mjs"
    },
    "./vite": {
      "types": "./dist/export-vite.d.ts",
      "require": "./dist/export-vite.js",
      "import": "./dist/export-vite.mjs"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json"
    }
  },
  "keywords": [
    "react",
    "react-dom",
    "next",
    "nextjs",
    "prev",
    "prevjs"
  ],
  "author": "zoubingwu",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.5",
    "@fabiospampinato/bump": "^2.1.2",
    "@types/etag": "^1.8.1",
    "@types/node": "^18.16.3",
    "@types/semver": "^7.5.0",
    "etag": "^1.8.1",
    "joycon": "^3.1.1",
    "strip-json-comments": "^5.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "bundle-require": "^4.0.1",
    "cac": "^6.7.14",
    "esbuild": "^0.17.5",
    "fast-glob": "^3.2.12",
    "just-camel-case": "^6.2.0",
    "just-capitalize": "^3.2.0",
    "semver": "^7.5.1",
    "vite": "^4.3.9",
    "wouter": "^2.10.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/cli.ts",
      "src/export-router.ts",
      "src/export-vite.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": true,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "external": [
      "react",
      "react-dom"
    ]
  }
}
